NODE_ENV=development

# API
PORT=4000
HOST=0.0.0.0
CLIME_API_KEYS=dev-local-key
CLIME_ADMIN_KEYS=dev-local-key
CLIME_REQUIRE_REGISTERED_API_KEYS=false
CLIME_RATE_LIMIT_PER_MINUTE=120
CLIME_SEARCH_RATE_LIMIT_PER_MINUTE=60
CLIME_REPORT_LIMIT_PER_HOUR=10
CLIME_CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
CLIME_API_LOGGER=true
# Optional override: local | openai (auto-detects openai when OPENAI_API_KEY is set)
# CLIME_EMBEDDING_PROVIDER=local
CLIME_OPENAI_EMBEDDING_MODEL=text-embedding-3-small
CLIME_EMBEDDING_TIMEOUT_MS=6000
CLIME_PGVECTOR_DIMENSION=384
CLIME_INSTALL_CHECKSUM_RESOLUTION=true
CLIME_CHECKSUM_CACHE_TTL_SECONDS=43200
CLIME_CHECKSUM_MAX_CACHE_ENTRIES=2000
CLIME_CHECKSUM_REQUEST_TIMEOUT_MS=10000
CLIME_CHECKSUM_MAX_DOWNLOAD_BYTES=83886080
CLIME_CHECKSUM_REFRESH_SCOPE=top
CLIME_CHECKSUM_REFRESH_LIMIT=250
CLIME_CHECKSUM_REFRESH_INTERVAL_SECONDS=86400
CLIME_CHECKSUM_REFRESH_ENABLED=true
DATABASE_URL=postgresql://localhost:5432/clime_dev
REDIS_URL=redis://localhost:6379/0

# Web
CLIME_API_BASE_URL=http://127.0.0.1:4000
NEXT_PUBLIC_API_URL=http://127.0.0.1:4000
CLIME_WEB_API_KEY=dev-local-key
NEXT_PUBLIC_GA_MEASUREMENT_ID=

# CLI
CLIME_API_KEY=dev-local-key
CLIME_BASE_URL=http://127.0.0.1:4000
CLIME_CACHE_TTL_SECONDS=180
